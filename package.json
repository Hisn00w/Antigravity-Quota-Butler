{
  "name": "antigravity-quota-butler",
  "displayName": "%ext.displayName%",
  "description": "%ext.description%",
  "version": "1.3.3",
  "publisher": "hisn00w",
  "icon": "assets/AntiGravityQuota.png",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./export/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ag-quota.refresh",
        "title": "%cmd.refresh%"
      },
      {
        "command": "ag-quota.reconnect",
        "title": "%cmd.reconnect%"
      },
      {
        "command": "ag-quota.show_logs",
        "title": "%cmd.logs%"
      },
      {
        "command": "ag-quota.show_dashboard",
        "title": "%cmd.dashboard%"
      }
    ],
    "configuration": {
      "title": "%config.title%",
      "properties": {
        "ag-quota.enabled": {
          "type": "boolean",
          "default": true,
          "description": "%config.enabled%",
          "order": 1
        },
        "ag-quota.pollingInterval": {
          "type": "number",
          "default": 120,
          "minimum": 30,
          "description": "%config.polling%",
          "order": 2
        },
        "ag-quota.pinnedModels": {
          "type": "array",
          "default": [],
          "items": {
            "type": "string"
          },
          "description": "%config.pinned%",
          "order": 3
        },
        "ag-quota.showPromptCredits": {
          "type": "boolean",
          "default": false,
          "description": "%config.credits%",
          "order": 4
        },
        "ag-quota.warningThreshold": {
          "type": "number",
          "default": 20,
          "minimum": 0,
          "maximum": 100,
          "description": "%config.threshold%",
          "order": 5
        },
        "ag-quota.enableNotifications": {
          "type": "boolean",
          "default": true,
          "description": "%config.notify%",
          "order": 6
        },
        "ag-quota.autoSwitchModels": {
          "type": "boolean",
          "default": false,
          "description": "%config.autoSwitch%",
          "order": 7
        },
        "ag-quota.enableResetNotification": {
          "type": "boolean",
          "default": false,
          "description": "%config.resetNotify%",
          "order": 8
        },
        "ag-quota.language": {
          "type": "string",
          "default": "auto",
          "enum": [
            "auto",
            "zh-cn",
            "en",
            "ja",
            "fr",
            "de"
          ],
          "enumDescriptions": [
            "Auto (Follow IDE)",
            "简体中文",
            "English",
            "日本語",
            "Français",
            "Deutsch"
          ],
          "description": "%config.language%",
          "order": 8
        }
      }
    }
  },
  "scripts": {
    "node:vsix:package": "npm run compile && npx vsce package",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vscode/vsce": "3.7.1",
    "eslint": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/Hisn00w/Antigravity-Quota-Butler.git"
  },
  "keywords": [
    "antigravity",
    "quota",
    "ai"
  ],
  "author": "Henrik Mertens",
  "license": "MIT"
}